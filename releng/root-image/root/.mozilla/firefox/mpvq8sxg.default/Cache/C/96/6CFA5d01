<script type="text/javascript">
//<![CDATA[
var brandLogo = document.querySelector("#brandLogo");
var cauldron = document.querySelector("#flame-wrap");
var flame = document.querySelector("#flame-wrap .fire");
var stopAni = window.setTimeout("flameOff()", 60000);
if(brandLogo){
  brandLogo.parentNode.insertBefore(cauldron, brandLogo.nextSibling);
  cauldron.style.display = "block";
}
function flameOn() {
  window.clearTimeout(stopAni); 
  cauldron.className = "ani-flame";
  stopAni = window.setTimeout("flameOff()", 60000);
}
function flameOff() { cauldron.className = ""; }
window.setTimeout("brandLogo.style.opacity = 0", 1000);
window.setTimeout("cauldron.style.opacity = 1", 1200);
cauldron.onmouseover = function(){ flameOn(); };
flameOn(); 
}
//]]>
</script>
